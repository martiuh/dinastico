    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "scripts": {
    "analyze:client": "yarn clean && cross-env ANAYLYZE=1 yarn build:client",
    "start": "yarn clean && cross-env NODE_ENV=development webpack-dev-server",
    "prod": "yarn build && yarn serve",
    "heroku-start": "node server.js",
    "clean": "rimraf public",
    "clean:chunks": "rimraf .routes && mkdir .routes",
    "build": "yarn clean && yarn build:chunks && yarn build:routes && yarn build:client && yarn build:estatico",
    "build:chunks": "yarn clean:chunks && babel-node estatico.build-chunks.js",
    "build:routes": "webpack --config estatico.routes.webpack.js && node .routes/buildRoutes",
    "build:client": "cross-env NODE_ENV=production webpack --mode production --colors",
    "build:estatico": "cross-env NODE_ENV=production webpack --mode production --config estatico.webpack.js",
    "serve": "cross-env NODE_ENV=production node server.js",
    "heroku-postbuild": "yarn build"
  }
}
