  "scripts": {
    "analyze:client": "yarn clean && yarn build:client --env ANALYZE",
    "start": "yarn clean && yarn clean:chunks && node devServer.js",
    "prod": "yarn build && yarn serve",
    "heroku-start": "node server.js",
    "clean": "rimraf public",
    "clean:chunks": "rimraf .routes && mkdir .routes",
    "build": "yarn clean && yarn build:chunks && yarn build:routes && yarn build:client && yarn build:dinastico",
    "build:chunks": "yarn clean:chunks && node dinastico.build-chunks.js",
    "build:routes": "webpack --config dinastico.routes.webpack.js --mode production && node .routes/buildRoutes",
    "build:client": "webpack --mode production --colors",
    "build:dinastico": "webpack --mode production --config dinastico.webpack.js",
